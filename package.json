{
  "name": "turnstyles",
  "version": "4.6.1",
  "main": "src/turnStyles.js",
  "repository": "git@github.com:pixelcrisis/turntable-tweaks.git",
  "author": "pixelcrisis <pxcrisis@gmail.com>",
  "license": "MIT",
  "scripts": {
    "build": "concurrently \"yarn b:index\" \"yarn b:theme\" \"yarn b:style\"",
    "watch": "concurrently \"yarn w:index\" \"yarn w:theme\" \"yarn w:style\"",
    "w:index": "watchify src/core/turnStyles.js -o dist/turnStyles.js",
    "b:index": "browserify -p tinyify src/core/turnStyles.js -o dist/turnStyles.js",
    "w:theme": "node-sass src/themes -wo dist/themes",
    "b:theme": "node-sass src/themes -o dist/themes",
    "w:style": "node-sass src/styles -wo dist/styles",
    "b:style": "node-sass src/styles -o dist/styles"
  },
  "devDependencies": {
    "babel-eslint": "^10.1.0",
    "browserify": "^17.0.0",
    "concurrently": "^6.0.0",
    "eslint": "^7.22.0",
    "node-sass": "^5.0.0",
    "sass": "^1.32.8",
    "tinyify": "^3.0.0",
    "watchify": "^4.0.0"
  }
}
